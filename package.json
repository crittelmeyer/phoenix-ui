{
  "name": "phoenix",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "husky": "^9.0.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "turbo": "^2.7.0"
  },
  "pnpm": {
    "overrides": {
      "react": "18.3.0",
      "react-dom": "18.3.0",
      "@types/react": "^18.3.0",
      "@types/react-dom": "^18.3.0"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,mdx,css}": [
      "prettier --write"
    ]
  }
}
